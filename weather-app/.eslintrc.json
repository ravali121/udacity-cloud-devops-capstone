{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
          "indent": ["error", 4],
          "no-unused-vars": "error",
          "quotes": [
            "error",
            "single"
          ],
          "semi": [
            "error",
            "always"
          ]          
    }
}
